{
    "name": "Slytherin-light",
    "type": "light",
    "colors": {
        "activityBar.background": "#205320",
        "activityBarBadge.background": "#cfcfcf",
        "activityBarBadge.foreground": "#000000",

        "badge.background": "#cfcfcf",
        "badge.foreground": "#000000",

        "breadcrumb.background": "#205320",
        "breadcrumb.focusForeground": "#cfcfcf",
        "breadcrumb.foreground": "#ccc2c2",
        "breadcrumbPicker.background": "#205320",

        "dropdown.background": "#f5f5f5",
        "dropdown.foreground": "#000000",

        "editor.background": "#e9e9e9",
        // "editor.background": "#ccf1d2",
        "editor.lineHighlightBackground": "#d4e6cf81",
        "editor.selectionBackground": "#e2e1e1",
        "editorCursor.foreground": "#205320",
        "editorGroupHeader.tabsBackground": "#458D45",
        "editorGutter.addedBackground": "#5a9c4a",
        "editorGutter.deletedBackground": "#961c1c",
        "editorGutter.modifiedBackground": "#608eb9",
        "editorLineNumber.activeForeground": "#6e6d6d",
        "editorLineNumber.foreground": "#acabab",
        "editorSuggestWidget.background": "#205320",
        "editorSuggestWidget.highlightForeground": "#dac16d",
        "editorSuggestWidget.selectedBackground": "#dbe8f394",
        "editorWidget.background": "#205320",
        "editorWidget.border": "#205320",

        // "foreground": "#fcfcfc",

        "diffEditor.insertedTextBackground": "#b1e2a7b6",
        "diffEditor.removedTextBackground": "#e2b5a7b6",

        "input.background": "#f5f5f5",
        "input.foreground": "#000000",
        "input.placeholderForeground": "#000000",

        "list.activeSelectionBackground": "#5ce03b96",
        "list.activeSelectionForeground": "#f5f5f5",
        "list.focusBackground": "#dbe8f394",
        "list.focusForeground": "#000000",
        "list.hoverBackground": "#dbe8f394",
        "list.hoverForeground": "#000000",
        "list.inactiveSelectionBackground": "#dcdfdbcb",
        "list.inactiveSelectionForeground": "#205320",

        "menu.background": "#205320",
        "menu.foreground": "#f5f5f5",
        "menu.selectionBackground": "#dbe8f394",
        "menu.selectionForeground": "#000000",

        "notificationCenterHeader.background": "#205320",
        "notifications.background": "#205320",

        "panelTitle.activeBorder": "#F0B600",
        // "panel.background": "#205320",
        "panel.background": "#E9E9E9",
        "panelTitle.activeForeground": "#205320",

        "pickerGroup.foreground": "#dac16d",

        "scrollbar.shadow": "#E9E9E9",

        "sideBar.background": "#458d45",
        // "sideBar.background": "#95b195",
        "sideBarSectionHeader.background": "#205320",
        "sideBarTitle.foreground": "#f1f1e0",

        "statusBar.background": "#205320",



        "tab.activeBackground": "#458d45",
        "tab.activeBorder": "#F0B600",
        "tab.border": "#458d45",
        "tab.inactiveBackground": "#458d45",
        "tab.inactiveForeground": "#ccc2c2",

        "titleBar.activeBackground": "#205320",

        "terminal.background": "#205320",

        "welcomePage.buttonBackground": "#E9E9E9",
        "welcomePage.buttonHoverBackground": "#205320",
    },
    "tokenColors": [{
            "scope": [
                "keyword",
                "keyword.control",
                "entity.name.tag.xml",
                "entity.other.attribute-name.xml",
                "entity.name.tag.namespace.xml",
                "source.php storage.type",
                "punctuation.definition.directive.c"
            ],
            "settings": {
                "foreground": "#d7571cf5"
            }
        },
        {
            "scope": [
                "comment",
                "comment.block",
                "source.powershell punctuation.definition",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#5c5c5c"
            }
        },
        {
            "scope": [
                "constant",
                "source.cs entity.name",
                "storage.modifier.import.java"
            ],
            "settings": {
                "foreground": "#9074b4"
            }
        },
        {
            "scope": [
                "string",
            ],
            "settings": {
                "foreground": "#856fa8"
            }
        },
        {
            "scope": [
                "variable",
                "keyword.other.variable.definition.powershell",
                "support.constant.automatic.powershell",
                "support.type.property-name.json",
                "entity.other.attribute-name",
                "meta.function-call.c",
                "punctuation.definition.variable.ruby",
                "punctuation.definition.variable.perl"
            ],
            "settings": {
                "foreground": "#be8316"
            }
        },
        {
            "scope": [
                "storage",
                "entity.name.function.coffee"
            ],
            "settings": {
                "foreground": "#5ea8cf"
            }
        },
        {
            "scope": "punctuation",
            "settings": {
                "foreground": "#8ba5d3"
            }
        },
        {
            "scope": [
                "entity",
                "variable.other.object.property.coffee"
            ],
            "settings": {
                "foreground": "#a78871"
            }
        },
        {
            "scope": [
                "support",
                "storage.type.cs",
                "entity.name.tag",
                "source.php storage.type.class",
                "source.php storage.type.function",
                "storage.modifier.java",
                "entity.name.function.c",
                "variable.other.object.coffee",
                "keyword.control.conditional.batchfile",
                "entity.name.type.class.module.erlang"
            ],
            "settings": {
                "foreground": "#BD2A1B"
            }
        },
        {
            "scope": [
                "meta",
                "string.quoted.double.json",
                "string.quoted.double.xml",
                "storage.modifier.ts",
                "source.cs keyword.control",
                "source.css keyword.other.unit",
                "source.php entity.name.type.class",
                "source.php entity.other.inherited-class",
                "entity.name.function.ruby",
                "entity.name.function.go",
                "entity.name.function.lua",
                "variable.other.field.erlang"
            ],
            "settings": {
                "foreground": "#218d4e"
            }
        },
        {
            "scope": [
                "text",
                "source.python"
            ],
            "settings": {
                "foreground": "#b69c76"
            }
        }
    ]
}